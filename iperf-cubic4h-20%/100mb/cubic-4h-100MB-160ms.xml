<?xml version="1.0"?>
<topology name="cubic-4h-100MB-160ms">
	<TCP-PROT>cubic</TCP-PROT>
	<duration>600</duration>
	<hosts>
		<host>h1</host>
		<host>h2</host>
		<host>h3</host>
		<host>h4</host>
	</hosts>
	<switches>
		<switch>s1</switch>
		<switch>s2</switch>
	</switches>
	<links>
		<link n1="h1" n2="s1"></link>
		<link n1="h2" n2="s1"></link>
		<link n1="h3" n2="s2"></link>
		<link n1="h4" n2="s2"></link>
		<backbone n1="s2" n2="s1">
			<bandwidth>100</bandwidth>
			<delay>160</delay>
		</backbone>
	</links>
	<iperf>
		<node type="server">h1</node>
		<node type="server">h2</node>
		<node type="client" delay="0" server="h1">h3</node>
		<node type="client" delay="100" server="h2">h4</node>
	</iperf>
</topology>
